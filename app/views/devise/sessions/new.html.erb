<div class="wrapper login has-navigation">
	<div class="content">
		<img class="logo-color" src="/images/logo-color.png" width="190" height="136" alt="Logo Color">
		<h2 class="form-header">Login</h2>
		<%= form_for(resource, :as => resource_name, :url => session_path(resource_name),
			:html => {:class => "form login-form" }) do |f| %>
			<%= f.text_field :username, :placeholder => "Username" %>
			<%= f.password_field :password, :placeholder => "Password" %>
			<%= f.submit "Login", {:class => "login-button button"} %>
		<%end%>
	</div>
</div>
